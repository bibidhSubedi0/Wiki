@model Tuple<NoteBoxModel,List<NoteMetadataModel>>

<h2>@Model.Item1.BoxName</h2>
<p>All notes inside this box:</p>

@foreach (var note in Model.Item2)
{
    <a asp-action="Index" asp-controller="Note" asp-route-noteGuid="@note.NoteGuid">
        @note.NoteName @note.NoteGuid @note.CreatedAt @note.UpdatedAt
    </a>

}   